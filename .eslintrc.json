{
    "extends": "standard",
    "rules": {
        "max-len": [2, 230,
            {
                "ignoreUrls": true,
                "ignoreComments": true
            }
        ],
        "camelcase": ["error", {"properties": "never"}],
        "indent": ["error", 4],
        "semi": [0, "always"]
    }
}


